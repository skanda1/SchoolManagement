<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/tle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="11dp"
        android:text="TextView"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/srtdte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/tle"
        android:layout_marginStart="152dp"
        android:text="TextView"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/endte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView48"
        android:layout_alignTop="@+id/tle"
        android:text="TextView"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/subdte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/srtdte"
        android:layout_alignTop="@+id/textView49"
        android:text="TextView"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView48"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/subdte"
        android:layout_marginEnd="22dp"
        android:text="edit"
        android:textColor="@android:color/holo_blue_bright"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/textView49"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="40dp"
        android:text="Submitted Date  :"
        android:textColor="@android:color/black"
        android:textSize="16sp" />

</RelativeLayout>
